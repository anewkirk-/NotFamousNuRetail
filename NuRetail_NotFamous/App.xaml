<Application x:Class="NuRetail_NotFamous.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:cont="clr-namespace:NuRetail_NotFamous.Controllers"
             xmlns:conv="clr-namespace:NuRetail_NotFamous.Converters"
             StartupUri="MainWindow.xaml"
             Exit="Application_Exit_1">
    <Application.Resources>
        <cont:QueryManager x:Key="QManager" ></cont:QueryManager>
        <conv:BoolToStatusbarTextConverter x:Key="StatusBarConv"></conv:BoolToStatusbarTextConverter>
        <conv:DoubleToMoneyStringConverter x:Key="MoneyConv"></conv:DoubleToMoneyStringConverter>
        <conv:UrlToImageConverter x:Key="UrlImgConv"></conv:UrlToImageConverter>
        </Application.Resources>
</Application>
